generator client {
  provider = "prisma-client-js"
}

generator erd{
  provider ="prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pool{
  id String @id @default(cuid())
  title String 
  codigo  String @unique
  createAt DateTime @default(now())

}